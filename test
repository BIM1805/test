UICorner_5.Parent = AntiAbusers

Nihilize.Name = "Nihilize"
Nihilize.Parent = MainFrame
Nihilize.BackgroundColor3 = Color3.fromRGB(255, 122, 61)
Nihilize.Position = UDim2.new(0.0694923103, 0, 0.731808424, 0)
