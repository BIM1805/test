-- Scripts:

local function CUYKCU_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	local UserInputService = game:GetService("UserInputService")

	local gui = script.Parent

	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			if gui.Visible then
				update(input)
			end
		end
	end)
end
coroutine.wrap(CUYKCU_fake_script)()
local function OOZRHN_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(OOZRHN_fake_script)()
local function FRQX_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	local subbtn = script.Parent
	local frame = script.Parent.Parent
	local passbox = script.Parent.Parent.TextBox
	local password = "2022"

	subbtn.MouseButton1Click:Connect(function()
		if passbox.Text == password then
			subbtn.Text = "CORRECT PASSWORD"
			wait(1)
			frame:TweenPosition(UDim2.new(5,0,0,0),"Out","Linear")
		else
			subbtn.Text = "INCORRECT PASSWORD"
			passbox.Text = ""
			wait(2)
			subbtn.Text = "SUBMIT"
		end
	end)
end
coroutine.wrap(FRQX_fake_script)()
local function KVNVREB_fake_script() -- TextLabel_11.Script 
	local script = Instance.new('Script', TextLabel_11)

	local text = {script.Parent.TextLabel}

	while wait() do
		for _,v in pairs(text) do
			local temp = v.Position.X.Scale
			if (temp <= -v.Size.X.Scale) then
				temp = 1.0
			end
			v.Position = UDim2.new(temp - 0.01, 0, 0.9, 0)
		end
	end
end
coroutine.wrap(KVNVREB_fake_script)()
local function NSTXBL_fake_script()
	local script = Instance.new('Script', TextLabel_12)

	while true do
		script.Parent.TextColor3 = Color3.new(1, 0.478431, 0.239216)
		wait(0.1)
	end
end
coroutine.wrap(NSTXBL_fake_script)()
